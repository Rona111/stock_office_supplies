<openerp>
    <data>
        <record model="ir.module.category" id="module_category_office_supplies">
            <field name="name">Office Supplies</field>
            <field name="sequence">1</field>
        </record>

        <record id="group_office_supplies_user" model="res.groups">
            <field name="name">Office Supplies / User</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="category_id" ref="stock_office_supplies.module_category_office_supplies"/>
        </record>
        <record id="group_office_supplies_manager" model="res.groups">
            <field name="name">Office Supplies / Manager</field>
            <field name="category_id" ref="stock_office_supplies.module_category_office_supplies"/>
            <field name="implied_ids" eval="[(4, ref('group_office_supplies_user'))]"/>
        </record>
    </data>
</openerp>